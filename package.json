{
  "name": "@eddy_h/toggle-minimize",
  "version": "1.0.1",
  "description": "To disable/enable minimizing by Win+D or show-desktop button, extends from electron-disable-minimize.",
  "main": "test.js",
  "types": "./types.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node-gyp build",
    "build:dev": "node-gyp build --debug",
    "cfg": "node-gyp configure",
    "dev": "electron-rebuild && electron .",
    "clean": "rimraf build bin",
    "compile": "npm run clean && npm run cfg && npm run build",
    "install": "node-gyp rebuild",
    "debug": "cross-env DEBUG=electron-rebuild electron-rebuild",
    "b": "npm run compile && npm run debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eddy0Hoang/toggle-minimize.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "electron",
    "windows disable minimize"
  ],
  "author": "Eddy Huang",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "nan": "^2.18.0",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-rebuild": "^3.2.9"
  },
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/Eddy0Hoang/toggle-minimize/issues"
  },
  "homepage": "https://github.com/Eddy0Hoang/toggle-minimize#readme"
}
